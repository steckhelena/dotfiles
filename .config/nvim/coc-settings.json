{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "javascript",
    "tsx",
    "graphql",
    "vue",
    "html",
    "javascriptreact",
    "typescript",
    "json",
    "python",
    "cpp",
    "c",
    "svelte"
  ],
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["-l", "80"]
}
