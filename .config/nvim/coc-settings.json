{
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"Markdown",
		"javascript",
		"tsx",
		"graphql",
		"vue",
		"html",
		"javascriptreact",
		"typescript",
		"json",
		"python",
		"cpp",
	],
	"prettier.eslintIntegration": true,
	"prettier.requireConfig": true,
	"eslint.autoFixOnSave": true,
	"python.formatting.provider": "black",
	"python.formatting.blackArgs": ["-l", "80"]
}
